<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="docview" />
		<Option execution_dir="." />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="win_gcc">
				<Option output="bin/$(TARGET_NAME)/$(PROJECT_NAME)" prefix_auto="1" extension_auto="1" />
				<Option object_output=".objs/$(TARGET_NAME)" />
				<Option type="0" />
				<Option compiler="gcc" />
				<Option projectLinkerOptionsRelation="2" />
				<Compiler>
					<Add option="`$(WX_CONFIG) --cflags`" />
				</Compiler>
				<Linker>
					<Add option="`$(WX_CONFIG) --libs base,core,adv`" />
					<Add directory="$(WXWIN)/lib/$(WXCFG)" />
				</Linker>
				<Environment>
					<Variable name="WX_CONFIG" value="wx-config --compiler=gcc --wxcfg=$(WXCFG)" />
				</Environment>
			</Target>
			<Target title="msys2_wx30_gcc">
				<Option output="bin/$(TARGET_NAME)/$(PROJECT_NAME)" prefix_auto="1" extension_auto="1" />
				<Option object_output=".objs/$(TARGET_NAME)" />
				<Option type="0" />
				<Option compiler="gcc" />
				<Option projectLinkerOptionsRelation="2" />
				<Compiler>
					<Add directory="$(TARGET_COMPILER_DIR)lib/wx/include/msw-unicode-$(WX_VERSION)" />
					<Add directory="$(TARGET_COMPILER_DIR)include/wx-$(WX_VERSION)" />
				</Compiler>
				<ResourceCompiler>
					<Add directory="$(TARGET_COMPILER_DIR)include/wx-$(WX_VERSION)" />
				</ResourceCompiler>
				<Linker>
					<Add library="wx_msw$(WX_SUFFIX)_adv-$(WX_VERSION).dll" />
					<Add library="wx_msw$(WX_SUFFIX)_core-$(WX_VERSION).dll" />
					<Add library="wx_base$(WX_SUFFIX)-$(WX_VERSION).dll" />
					<Add directory="$(WXWIN)/lib/$(WXCFG)" />
				</Linker>
				<Environment>
					<Variable name="WX_SUFFIX" value="u" />
					<Variable name="WX_VERSION" value="3.0" />
				</Environment>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="win_gcc;" />
		</VirtualTargets>
		<ResourceCompiler>
			<Add directory="%WXWIN%/include" />
			<Add directory=".." />
		</ResourceCompiler>
		<Unit filename="chart.ico" />
		<Unit filename="chart.xpm" />
		<Unit filename="doc.cpp" />
		<Unit filename="doc.h" />
		<Unit filename="doc.ico" />
		<Unit filename="doc.xpm" />
		<Unit filename="docview.cpp" />
		<Unit filename="docview.h" />
		<Unit filename="docview.rc">
			<Option compilerVar="WINDRES" />
		</Unit>
		<Unit filename="notepad.ico" />
		<Unit filename="notepad.xpm" />
		<Unit filename="view.cpp" />
		<Unit filename="view.h" />
		<Extensions>
			<code_completion />
			<envvars />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
