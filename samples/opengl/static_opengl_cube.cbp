<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="static_opengl_cube" />
		<Option execution_dir="." />
		<Option pch_mode="2" />
		<Option compiler="null" />
		<Build>
			<Target title="win_gcc">
				<Option output="bin/$(TARGET_NAME)/$(PROJECT_NAME)" prefix_auto="1" extension_auto="1" />
				<Option object_output=".objs/$(TARGET_NAME)" />
				<Option type="0" />
				<Option compiler="gcc" />
				<Option projectLinkerOptionsRelation="2" />
				<Compiler>
					<Add option="`$(WX_CONFIG) --cflags`" />
				</Compiler>
				<Linker>
					<Add option="`$(WX_CONFIG) --libs std,gl`" />
				</Linker>
				<Environment>
					<Variable name="WX_CONFIG" value="wx-config --compiler=gcc --wxcfg=$(WXCFG)" />
				</Environment>
			</Target>
			<Target title="msys2_wx30_gcc">
				<Option output="bin/$(TARGET_NAME)/$(PROJECT_NAME)" prefix_auto="1" extension_auto="1" />
				<Option object_output=".objs/$(TARGET_NAME)" />
				<Option type="0" />
				<Option compiler="msys2-gcc" />
				<Option projectLinkerOptionsRelation="2" />
				<Compiler>
					<Add option='-DWXDLLIMPEXP_GL=&quot;&quot;' />
					<Add directory="$(TARGET_COMPILER_DIR)lib/wx/include/msw-unicode-static-$(WX_VERSION)" />
					<Add directory="$(TARGET_COMPILER_DIR)include/wx-$(WX_VERSION)" />
				</Compiler>
				<Linker>
					<Add library="$(TARGET_COMPILER_DIR)lib/libwx_msw$(WX_SUFFIX)_gl-$(WX_VERSION).a" />
					<Add library="$(TARGET_COMPILER_DIR)lib/libwx_msw$(WX_SUFFIX)_core-$(WX_VERSION).a" />
					<Add library="$(TARGET_COMPILER_DIR)lib/libwx_base$(WX_SUFFIX)-$(WX_VERSION).a" />
					<Add library="opengl32" />
					<Add library="png" />
					<Add library="oleaut32" />
					<Add library="ole32" />
					<Add library="uuid" />
					<Add library="winspool" />
					<Add library="comctl32" />
					<Add library="oleacc" />
					<Add directory="$(TARGET_COMPILER_DIR)lib" />
				</Linker>
				<Environment>
					<Variable name="WX_SUFFIX" value="u" />
					<Variable name="WX_VERSION" value="3.0" />
				</Environment>
			</Target>
			<Target title="msys2_wx31_gcc">
				<Option output="bin/$(TARGET_NAME)/$(PROJECT_NAME)" prefix_auto="1" extension_auto="1" />
				<Option object_output=".objs/$(TARGET_NAME)" />
				<Option type="0" />
				<Option compiler="msys2-gcc" />
				<Option projectLinkerOptionsRelation="2" />
				<Compiler>
					<Add directory="$(TARGET_COMPILER_DIR)lib/wx/include/msw-unicode-$(WX_VERSION)" />
					<Add directory="$(TARGET_COMPILER_DIR)include/wx-$(WX_VERSION)" />
				</Compiler>
				<Linker>
					<Add library="$(TARGET_COMPILER_DIR)lib/libwx_msw$(WX_SUFFIX)_gl-$(WX_VERSION).a" />
					<Add library="$(TARGET_COMPILER_DIR)lib/libwx_msw$(WX_SUFFIX)_core-$(WX_VERSION).a" />
					<Add library="$(TARGET_COMPILER_DIR)lib/libwx_base$(WX_SUFFIX)-$(WX_VERSION).a" />
					<Add library="opengl32" />
					<Add library="png" />
					<Add library="oleaut32" />
					<Add library="ole32" />
					<Add library="uuid" />
					<Add library="winspool" />
					<Add library="shlwapi" />
					<Add library="comctl32" />
					<Add library="version" />
					<Add library="oleacc" />
				</Linker>
				<Environment>
					<Variable name="WX_SUFFIX" value="u" />
					<Variable name="WX_VERSION" value="3.1" />
				</Environment>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="win_gcc;msys2_wx30_gcc;msys2_wx31_gcc;" />
			<Add alias="MSys2" targets="msys2_wx30_gcc;msys2_wx31_gcc;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
		</Compiler>
		<ResourceCompiler>
			<Add directory="%WXWIN%/include" />
		</ResourceCompiler>
		<Unit filename="cube/cube.cpp" />
		<Unit filename="cube/cube.h" />
		<Extensions>
			<code_completion />
			<envvars />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
