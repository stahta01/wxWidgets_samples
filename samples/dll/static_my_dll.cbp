<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="static_my_dll" />
		<Option platforms="Windows;" />
		<Option execution_dir="." />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="win_gcc">
				<Option output="bin/$(TARGET_NAME)/my_dll" imp_lib="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).a" def_file="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).def" prefix_auto="1" extension_auto="1" />
				<Option object_output=".objs/$(TARGET_NAME)" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createStaticLib="1" />
				<Option projectLinkerOptionsRelation="2" />
				<Compiler>
					<Add option="`$(WX_CONFIG) --cflags`" />
				</Compiler>
				<Linker>
					<Add option="`$(WX_CONFIG) --libs base,core,adv`" />
					<Add directory="$(WXWIN)/lib/$(WXCFG)" />
				</Linker>
				<Environment>
					<Variable name="WX_CONFIG" value="wx-config --compiler=gcc --wxcfg=$(WXCFG)" />
				</Environment>
			</Target>
			<Target title="msys2_wx30_gcc">
				<Option output="bin/$(TARGET_NAME)/my_dll" imp_lib="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).a" def_file="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).def" prefix_auto="1" extension_auto="1" />
				<Option object_output=".objs/$(TARGET_NAME)" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createStaticLib="1" />
				<Option projectLinkerOptionsRelation="2" />
				<Compiler>
					<Add directory="$(TARGET_COMPILER_DIR)lib/wx/include/msw-unicode-static-$(WX_VERSION)" />
					<Add directory="$(TARGET_COMPILER_DIR)include/wx-$(WX_VERSION)" />
				</Compiler>
				<Linker>
					<Add library="$(TARGET_COMPILER_DIR)lib/libwx_msw$(WX_SUFFIX)_core-$(WX_VERSION).a" />
					<Add library="$(TARGET_COMPILER_DIR)lib/libwx_base$(WX_SUFFIX)-$(WX_VERSION).a" />
					<Add library="png" />
					<Add library="oleaut32" />
					<Add library="ole32" />
					<Add library="uuid" />
					<Add library="winspool" />
					<Add library="comctl32" />
					<Add library="comdlg32" />
					<Add library="gdi32" />
					<Add library="oleacc" />
				</Linker>
				<Environment>
					<Variable name="WX_SUFFIX" value="u" />
					<Variable name="WX_VERSION" value="3.0" />
				</Environment>
			</Target>
			<Target title="msys2_wx31_gcc">
				<Option output="bin/$(TARGET_NAME)/my_dll" imp_lib="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).a" def_file="$(TARGET_OUTPUT_DIR)$(TARGET_OUTPUT_BASENAME).def" prefix_auto="1" extension_auto="1" />
				<Option object_output=".objs/$(TARGET_NAME)" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createStaticLib="1" />
				<Option projectLinkerOptionsRelation="2" />
				<Compiler>
					<Add directory="$(TARGET_COMPILER_DIR)lib/wx/include/msw-unicode-static-$(WX_VERSION)" />
					<Add directory="$(TARGET_COMPILER_DIR)include/wx-$(WX_VERSION)" />
				</Compiler>
				<Linker>
					<Add library="$(TARGET_COMPILER_DIR)lib/libwx_msw$(WX_SUFFIX)_core-$(WX_VERSION).a" />
					<Add library="$(TARGET_COMPILER_DIR)lib/libwx_base$(WX_SUFFIX)-$(WX_VERSION).a" />
					<Add library="png" />
					<Add library="oleaut32" />
					<Add library="ole32" />
					<Add library="uuid" />
					<Add library="winspool" />
					<Add library="shlwapi" />
					<Add library="comctl32" />
					<Add library="comdlg32" />
					<Add library="version" />
					<Add library="gdi32" />
					<Add library="oleacc" />
				</Linker>
				<Environment>
					<Variable name="WX_SUFFIX" value="u" />
					<Variable name="WX_VERSION" value="3.1" />
				</Environment>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="win_gcc;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-DMY_DLL_BUILDING" />
		</Compiler>
		<Unit filename="my_dll.cpp" />
		<Unit filename="my_dll.h" />
		<Extensions>
			<code_completion />
			<envvars />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
